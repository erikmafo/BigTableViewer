<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.erikmafo.btviewer.ui.dialogs.tablesettings.CellDefinitionViewController"
      prefHeight="400.0"
      prefWidth="600.0"
      spacing="2">
    <TextField fx:id="familyTextField" promptText="family"/>
    <TextField fx:id="qualifierTextField" promptText="qualifier pattern"/>
    <ComboBox fx:id="valueTypeChoiceBox" promptText="Select value type"/>
    <Button fx:id="configureProtoObjButton"/>
    <Button fx:id="deleteButton"/>
</HBox>
